<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="chat.client.chatclient.SignUpController">
   <children>
      <TextField fx:id="loginField" layoutX="23.0" layoutY="200.0" promptText="login" />
      <Button fx:id="signUpBtn" layoutX="168.0" layoutY="259.0" mnemonicParsing="false" text="Sign up" />
      <Button fx:id="signInBtn" layoutX="308.0" layoutY="40.0" mnemonicParsing="false" text="Sign in" />
      <PasswordField fx:id="passwordField" layoutX="217.0" layoutY="200.0" promptText="password" />
      <TextField fx:id="firstNameField" layoutX="23.0" layoutY="137.0" promptText="first name" />
      <TextField fx:id="lastNameField" layoutX="217.0" layoutY="137.0" promptText="last name" />
   </children>
</AnchorPane>
